{
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "manifest_version": 2,
    "name": "Packlist",
    "version": "0.0.1",
    "description": "An application for assembling the packing list for your next outing",
    "icons": {},
    "oauth2": {
        "client_id": "588205410918.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/drive"
        ]
    },
    "offline_enabled": true,
    "permissions": [
        "https://docs.google.com/feeds/",
        "https://docs.googleusercontent.com/",
        "https://spreadsheets.google.com/feeds",
        "https://ssl.gstatic.com/",
        "https://www.googleapis.com/"
    ]
}